{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.1",
    "@mdx-js/react": "^1.6.1",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.21.28",
    "gatsby-plugin-google-analytics": "^2.3.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-mdx": "^1.2.7",
    "gatsby-plugin-nprogress": "^2.3.1",
    "gatsby-plugin-page-creator": "^2.3.2",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sass": "^2.1.20",
    "gatsby-plugin-sitemap": "^2.4.2",
    "gatsby-plugin-styled-components": "^3.3.1",
    "gatsby-source-filesystem": "^2.3.3",
    "gatsby-transformer-remark": "^2.8.8",
    "node-sass": "^4.14.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^6.0.0",
    "react-switch": "^5.0.1",
    "react-use": "^13.2.1",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
